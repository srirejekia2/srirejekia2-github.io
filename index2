		<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-geWF76RCwLtnZ8qwWowPQNguL3RmwHVBC9FhGdlKrxdiJJigb/j/68SIy3Te4Bkz" crossorigin="anonymous"></script>
		<script>
			let htm = '';
			const data = {
				jenis: [
					{
						tipe: 'Heavy Meal',
						macam: [
							{ nama: 'Nasi Goreng Original', harga: '15K' },
							{ nama: 'Nasi Goreng Pedas', harga: '20K' },
							{ nama: 'Mie Goreng Original', harga: '15K' },
							{ nama: 'Mie Goreng Pedas', harga: '20K' },
						],
					},
					{
						tipe: 'Snack',
						macam: [
							{ nama: 'Dimsum', harga: '15K' },
							{ nama: 'Pentol', harga: '20K' },
							{ nama: 'Kentang', harga: '15K' },
							{ nama: 'Roti Goreng', harga: '20K' },
						],
					},
				],
			};

			let x = data.jenis;
			for (let i in x) {
				htm += '<section id="' + x[i].tipe + '" style="padding-top: 3.5rem">';
				htm += '<h2 class="text-white text-center py-2" style="background-color: #476040">' + x[i].tipe + '</h2><div class="row">';
				let y = x[i].macam;
				for (let j in y) {
					htm += '<div class="col-sm-3 mb-3 mb-sm-0"><div class="card bg-warning-subtle">';
					htm += '<img src="img/milkshake-2.jpg" class="card-img-top p-2" alt="' + y[j] + '" />';
					htm += '<div class="card-body"><h5 class="card-title">' + y[j].nama + ' <span class="badge rounded-pill text-bg-primary">' + y[j].harga + '</span></h5></div>';
					htm += '</div></div>';
				}
				htm += '</section>';
			}
			document.getElementById('coba').innerHTML = htm;
		</script>
	</body>
